{
  "name": "PySR",
  "key": "pysr",
  "python_version": "3.10.13",
  "install_commands": [
    "mkdir deps",
    "cd deps && wget https://julialang-s3.julialang.org/bin/linux/x64/1.9/julia-1.9.4-linux-x86_64.tar.gz",
    "cd deps && tar zxvf julia-1.9.4-linux-x86_64.tar.gz",
    "cd deps && rm julia-1.9.4-linux-x86_64.tar.gz",
    "export PATH=\"$PATH:deps/julia-1.9.4/bin\"",
    "pip install -U pysr",
    "python -m pysr install"
  ]
}